<?xml version='1.0' encoding='utf-8'?>
<widget id="com.livechat.dating" version="0.0.1">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="io.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="BackgroundServicePlugin">
        <param name="android-package" value="com.red_folder.phonegap.plugin.backgroundservice.BackgroundServicePlugin" />
    </feature>
    <feature name="org.apache.cordova.facebook.Connect">
        <param name="android-package" value="org.apache.cordova.facebook.ConnectPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="com.livechat.dating.Device" />
    </feature>
    <feature name="Share">
        <param name="android-package" value="com.livechat.dating.Share" />
    </feature>
    <feature name="Home">
        <param name="android-package" value="com.livechat.dating.Home" />
    </feature>
    <feature name="Toast">
        <param name="android-package" value="com.livechat.dating.Toast" />
    </feature>
    <feature name="Confirm">
        <param name="android-package" value="com.livechat.dating.Confirm" />
    </feature>
    <feature name="TextArea">
        <param name="android-package" value="com.livechat.dating.TextArea" />
    </feature>
    <preference name="loglevel" value="DEBUG" />
    <preference name="fullscreen" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="FadeSplashScreenDuration" value="0" />
    <description>
        Meet people, Make friends.
    </description>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>LiveChat</name>
    <description>
        A sample chat app
    </description>
    <feature name="PDialog">
        <param name="android-package" value="io.github.pwlin.cordova.plugins.pdialog.PDialog" />
    </feature>
    <allow-intent href="market:*" />
    <name>Live Chat</name>
    <description>
        A sample Live Chat.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Meet App team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="connectivity">
        <param name="android-package" value="com.appfeel.cordova.connectivity.Connectivity" />
    </feature>
    <feature name="AdMobAds">
        <param name="android-package" value="com.appfeel.cordova.admob.AdMobAds" />
    </feature>
    <feature name="GooglePlus">
        <param name="android-package" value="nl.xservices.plugins.GooglePlus" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://accounts.google.com/*" />
</widget>
